<div class="d-flex flex-column align-items-center justify-content-center " id="mainContent">
  <div id="carouselIndicators" class="carousel slide carousel-fade w-100 shadow-lg" data-bs-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselIndicators" data-slide-to="1"></li>
      <li data-target="#carouselIndicators" data-slide-to="2"></li>
      <li data-target="#carouselIndicators" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <%= image_tag("North-Hlth-Sci-Bldg900x416.jpeg", class: "d-block w-100", alt: "North Seattle Campus") %>
      </div>
      <div class="carousel-item">
        <%= image_tag("placeholder2.png", class: "d-block w-100", alt: "Android") %>
      </div>
      <div class="carousel-item">
        <%= image_tag("placeholder.png", class: "d-block w-100", alt: "Career") %>
      </div>
      <div class="carousel-item">
        <%= image_tag("placeholder2.png", class: "d-block w-100", alt: "Ethics") %>
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>

  <div class="d-flex flex-column align-items-center justify-content-center"
       id="mainContent"
       style="background-image: url('../../assets/images/paper-texture-structure-stained-paper.jpeg')">

    <div class="w-75 d-flex justify-content-around shadow-lg bg-white p-3" id="community">
      <div class="container col-11" height="500">
        <div class="d-flex align-items-center justify-content-around row">
          <div class="col-5">
            <%= image_tag("DSCLOGO.png", class: "d-block w-100", alt: "Google Developer Logo") %>
          </div>
          <div class="col-7" >
            <div class="card w-100 d-flex align-items-center justify-content-center shadow bg-white p-3 m-4">
              <iframe width="95%" height="300" src="https://www.youtube.com/embed/earTjC0iSjg" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
          </div>
        </div>
        <div class="d-flex align-items-center justify-content-around row" id="discord">
          <div class="col" >
            <div class="card w-100 shadow bg-white p-3 m-4">
              <iframe src="https://discord.com/widget?id=564559681339785228&theme=dark"
                      width="100%"
                      height="350"
                      allowtransparency="true"
                      frameborder="0"
                      sandbox="allow-popups allow-popups-to-escape-sandbox allow-same-origin allow-scripts">
              </iframe>

            </div>
          </div>

        </div>
      </div>
    </div>

