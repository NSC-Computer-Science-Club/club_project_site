          <div class="col-md-4" id="core-team-cards">
            <div class="card border-warning w-100 shadow-lg bg-white p-3" style="width: 300px;">
                <%= image_tag("GD.png", class: "card-img-top", alt: "sample image", style: "height: 200px;") %>
                <div class="card-body text-center">
                    <h6 class="card-title">Jardi Martinez Jordan</h6>
                    <p class="card-text">Technical Lead</p>
                </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- holding us up away from the footer -->
    <div class="w-75 d-flex justify-content-center shadow-lg bg-white p-3" style="height: 110px; min-width: 90%;">
    </div>

  </div>
