<div class="d-flex flex-column align-items-center justify-content-center" id="mainContent">
  <div id="carouselIndicators" class="carousel slide carousel-fade w-100" data-bs-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselIndicators" data-slide-to="1"></li>
      <li data-target="#carouselIndicators" data-slide-to="2"></li>
      <li data-target="#carouselIndicators" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <%= image_tag("placeholder.png", class: "d-block w-100", alt: "Community") %>
      </div>
      <div class="carousel-item">
        <%= image_tag("placeholder2.png", class: "d-block w-100", alt: "Android") %>
      </div>
      <div class="carousel-item">
        <%= image_tag("placeholder.png", class: "d-block w-100", alt: "Career") %>
      </div>
      <div class="carousel-item">
        <%= image_tag("placeholder2.png", class: "d-block w-100", alt: "Ethics") %>
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>

  <div data-bs-spy="scroll" data-bs-target="#nav" data-bs-offset="0" tabindex="0" class="d-flex flex-column align-items-center justify-content-center" id="mainContent">

    <div class="w-75 d-flex justify-content-around shadow bg-white p-3" id="community">
