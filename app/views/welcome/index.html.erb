<div class="d-flex flex-column align-items-center justify-content-center" >

  <%= image_tag("North-Hlth-Sci-Bldg900x416.jpg", class: "d-block w-100", alt: "North Seattle Campus") %>


  <div class="d-flex flex-column align-items-center justify-content-center"
       id="mainContent" >
    <div class="w-75 d-flex justify-content-around shadow-lg bg-white p-3" id="community" style="padding-top: 115px; min-width: 90%;">
      <div class="container-fluid col-md-12" height="500">
        <div class="d-flex justify-content-md-center align-items-md-center row mb-5" id="discord">
          <div class="col-md-6 text-center">
            <h1 style="margin-top: 20px;">We're a <span class="rounded" style="color: #4285F4; padding: 5px;">community</span> of students who came together... </h1>
          </div>
          <div class="col col-md-6" >
            <div class="card w-100 shadow bg-white p-3">
              <iframe src="https://discord.com/widget?id=564559681339785228&theme=dark"
                      width="100%"
                      height="350"
                      allowtransparency="true"
                      frameborder="0"
                      sandbox="allow-popups allow-popups-to-escape-sandbox allow-same-origin allow-scripts">
              </iframe>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="w-75 d-flex justify-content-center shadow-lg bg-white" id="events" style="padding-top: 115px; min-width: 90%;">
      <div class="container-fluid col-md-12">
        <div class="row mb-5 justify-content-center text-center">
          <h1 class="my-5">...looking to build <span class="rounded" style="color: #EA4335; padding: 5px;">skills</span>...</h1>
        </div>
        <div class="row mb-5">
          <%= image_tag("allthetech.png", class: "d-block w-100 ml-3", alt: "Firebase Logo") %>
        </div>
      </div>

    </div>

